<?xml version="1.0"?>

<group title="BlinkModule Example">

		<module
			class = "Constant"
			name = "MyConstant"
			outputsize  = "2"
			data  = "1 2"
		/>

		<module
			class = "BlinkModule"
			name = "BlinkModuleInstance"
			parameter1  = "1.23"
			parameter2  = "12"
		/>

        <connection sourcemodule = "MyConstant" source = "OUTPUT" targetmodule = "BlinkModuleInstance" target = "INPUT1" />
        <connection sourcemodule = "MyConstant" source = "OUTPUT" targetmodule = "BlinkModuleInstance" target = "INPUT2" />

		<view name="View">
			<object class="BarGraph" module="BlinkModuleInstance" source="OUTPUT1" x="0" y="0" />
			<object class="BarGraph" module="BlinkModuleInstance" source="OUTPUT2" x="1" y="0" />
		</view>

</group>
